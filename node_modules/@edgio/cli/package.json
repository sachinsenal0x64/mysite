{
  "name": "@edgio/cli",
  "version": "7.7.0",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf ./dist; mkdir ./dist; cp package.json README.md ./dist; babel src -d dist",
    "watch": "npm-watch",
    "push-build": "npm run build && cd dist && yalc push && cd ..",
    "test": "jest",
    "release": "cd ./dist; npm publish --access public"
  },
  "watch": {
    "push-build": {
      "patterns": [
        "src",
        "package.json"
      ],
      "extensions": "js",
      "quiet": false
    }
  },
  "dependencies": {
    "axios": "^1.6.0",
    "chalk": "^4.1.2",
    "check-node-version": "^4.2.1",
    "cli-progress": "^3.9.1",
    "command-exists": "^1.2.9",
    "conf": "^6.1.0",
    "cross-spawn": "^7.0.3",
    "date-fns": "^2.27.0",
    "decode-uri-component": "^0.2.0",
    "execa": "^3.2.0",
    "filesize": "^6.0.1",
    "form-data": "^2.5.1",
    "fs-extra": "^8.1.0",
    "git-url-parse": "^13.1.0",
    "globby": "^10.0.1",
    "https-proxy-agent": "^7.0.2",
    "import-local": "^3.0.2",
    "ipaddr.js": "^2.0.1",
    "is-ci": "^2.0.0",
    "jscodeshift": "^0.13.1",
    "json5": "^2.2.2",
    "kill-port": "^1.6.1",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "mixpanel": "^0.17.0",
    "node-fetch": "^2.6.7",
    "normalize-url": "^6.1.0",
    "open": "^7.0.0",
    "ora": "^4.0.3",
    "prettier": "^2.5.0",
    "prompts": "^2.4.2",
    "resolve-global": "^1.0.0",
    "resolve-package-path": "^4.0.3",
    "semver": "^7.3.5",
    "sharp": "^0.31.2",
    "simple-git": "^3.16.0",
    "terminal-link": "^2.1.1",
    "tmp-promise": "^2.0.2",
    "ws": "^7.4.6",
    "yargs": "^16.2.0",
    "yauzl": "^2.10.0",
    "yazl": "^2.5.1"
  },
  "devDependencies": {
    "@edgio/core": "^7.7.0",
    "@edgio/next": "^7.7.0"
  },
  "bin": {
    "edg": "./index.js",
    "edgio": "./index.js"
  },
  "publishConfig": {
    "directory": "dist"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "jest": {
    "clearMocks": true,
    "collectCoverage": true,
    "testMatch": [
      "**/test/**/*.test.js"
    ],
    "transform": {
      "^.+\\.(js)?$": "babel-jest"
    },
    "setupFiles": [
      "<rootDir>/test/setEnvVars.js"
    ],
    "collectCoverageFrom": [
      "./src/frameworks/**/*.{js,jsx}",
      "./src/serverless/**/*.{js,jsx}",
      "./src/utils/**/getEntryPoint.js",
      "./src/utils/**/time.js",
      "./src/utils/config/**/*.js",
      "./src/curl/**/*.js",
      "!./src/curl/options/*.js",
      "!**/test/**",
      "!**/index.js"
    ],
    "moduleNameMapper": {
      "^@edgio/core(.*)$": "<rootDir>/../core/src/$1",
      "^@edgio/next(.*)$": "<rootDir>/../next/src/$1"
    }
  }
}
